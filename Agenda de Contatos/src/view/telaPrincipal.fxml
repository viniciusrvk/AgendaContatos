<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.telaPrincipalController">
   <children>
      <MenuBar prefHeight="30.0" prefWidth="500.0">
        <menus>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#addNew" text="Add new" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ListView fx:id="listContatos" layoutY="30.0" prefHeight="470.0" prefWidth="156.0" />
      <Label layoutX="250.0" layoutY="45.0" prefHeight="30.0" text="Contato" />
      <Label fx:id="nome" layoutX="250.0" layoutY="85.0" prefHeight="30.0" />
      <Label fx:id="telefone" layoutX="250.0" layoutY="164.0" prefHeight="30.0" />
      <Label fx:id="email" layoutX="250.0" layoutY="125.0" prefHeight="30.0" />
      <Button fx:id="get" layoutX="171.0" layoutY="451.0" mnemonicParsing="false" onAction="#lvSelect" text="show selected" />
   </children>
</AnchorPane>
